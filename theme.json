		"custom": {
			"lineHeight": {
				"xs": "1.25rem",
				"sm": "1.5rem",
				"base": "1.5rem",
				"lg": "clamp( 1.75rem, 2.1875vw + 1.00625rem, 1.96875rem )",
				"xl": "1.75",
				"2-xl": "clamp( 2.1875rem, -1.875vw + 2.825rem, 2rem )",
				"3-xl": "clamp( 2rem,2.5vw + 1.15rem, 2.25rem )",
				"4-xl": "clamp( 2.125rem, 4.0625vw + 0.74375rem, 2.53125rem )",
				"5-xl": "clamp( 2.25rem, 5.55556vw + 0.36111rem, 3.25rem )"
			}
		},
                "layout": {
                        "contentSize": "40rem",
                        "wideSize": "64rem"
                },
		"shadow": {
			"presets": [
				{
					"name": "Small",
					"slug": "sm",
					"shadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)"
				},
				{
					"name": "Medium",
					"slug": "md",
					"shadow": "0 4px  10px 0 rgba( 0, 0, 0, 0.3 )"
				},
				{
					"name": "Large",
					"slug": "lg",
					"shadow": "0 8px  15px 0 rgba( 0, 0, 0, 0.3 )"
				}
			]
		},
                "spacing": {
                        "blockGap": true,
			"customSpacingSize": true,
                        "margin": true,
                        "padding": true,
			"spacingSizes": [
				{
					"name": "Pixel - Fixed (1px)",
					"size": "1px",
					"slug": "px"
				},
				{
					"name": "Step 1 - Fixed (0.25rem)",
					"size": "0.25rem",
					"slug": "10"
