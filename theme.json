				{
					"name": "Step 7 - Fixed (1.75rem)",
					"size": "1.75rem",
					"slug": "70"
				},
				{
					"name": "Step 8 - Fixed (2rem)",
					"size": "2rem",
					"slug": "80"
				},
				{
					"name": "Step 9 - Fluid (2.25rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 1.0417)), 2.25rem)",
					"slug": "90"
				},
				{
					"name": "Step 10 - Fluid (2.5rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 2.0833)), 2.5rem)",
					"slug": "100"
				},
				{
					"name": "Step 12 - Fluid (3rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 4.1667)), 3rem)",
					"slug": "120"
				},
				{
					"name": "Step 16 - Fluid (4rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 8.3333)), 4rem)",
					"slug": "160"
				},
				{
					"name": "Step 24 - Fluid (6rem)",
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 8.3333)), 6rem)",
					"slug": "240"
				},
				{
					"name": "Step 32 - Fluid (8rem)",
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 16.6667)), 8rem)",
					"slug": "320"
				}
			],
                        "units": [
                                "rem",
                                "em",
                                "%",
                                "vh",
                                "vw",
                                "px"
                        ]

                },
                "typography": {
                        "customFontSize": false,
                        "dropCap": true,
                        "fontFamilies": [
                                {
                                        "fontFamily": "inherit",
                                        "slug": "headings",
                                        "name": "Headings"
                                },
                                {
                                        "fontFamily": "Karla, sans-serif",
                                        "slug": "primary",
                                        "name": "Primary",
					"fontFace": [
						{
							"fontFamily": "Karla",
							"fontWeight": "400 700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/karla/karla.woff2" ]
						},
						{
							"fontFamily": "Karla",
							"fontWeight": "400 700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/karla/karla-italic.woff2" ]
						}
					]
                                },
                                {
                                        "fontFamily": "Lora, Georgia, serif",
                                        "slug": "secondary",
                                        "name": "Secondary"
                                },
                                {
                                        "fontFamily": "\"Source Code Pro\", Monaco, Consolas, \"Andale Mono WT\", \"Andale Mono\", \"Lucida Console\", \"Lucida Sans Typewriter\", \"DejaVu Sans Mono\", \"Bitstream Vera Sans Mono\", \"Liberation Mono\", \"Nimbus Mono L\", \"Courier New\", Courier, monospace",
                                        "slug": "mono",
                                        "name": "Monospace"
                                }
                        ],
			"fontSizes": [
				{
					"name": "XS",
					"size": "0.875rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "0.9375rem",
					"slug": "sm"
				},
				{
					"name": "Medium (Base)",
					"size": "1rem",
					"slug": "base"
				},
				{
					"name": "Large",
					"size": "clamp( 1.0625rem, 0.625vw + 0.85rem, 1.125rem )",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "clamp( 1.125rem, 1.25vw + 0.7rem, 1.25rem )",
					"slug": "xl"
				},
				{
					"name": "2XL",
					"size": "clamp( 1.25rem, 2.5vw + 0.4rem, 1.5rem )",
					"slug": "2-xl"
				},
				{
					"name": "3XL",
					"size": "clamp( 1.5rem, 3.75vw + 0.225rem, 1.875rem )",
					"slug": "3-xl"
				},
				{
					"name": "4XL",
					"size": "clamp( 1.75rem, 5vw + 0.05rem, 2.25rem )",
					"slug": "4-xl"
				},
				{
					"name": "5XL",
					"size": "clamp( 2rem, 5.55556vw + 0.11111rem, 3rem )",
					"slug": "5-xl"
				}
			],
                        "fontStyle": true,
                        "fontWeight": true,
                        "letterSpacing": true,
                        "lineHeight": false,
                        "textDecoration": true,
                        "textTransform": true
                },
		"useRootPaddingAwareAlignments": true
        },
        "styles": {
                "color": {
                        "text": "var( --wp--preset--color--neutral-900 )",
                        "background": "var( --wp--preset--color--white )"
                },
                "spacing": {
                        "blockGap": "2rem",
                        "padding": {
				"top": "0",
				"bottom": "0",
				"left": "2rem",
				"right": "2rem"
			}
                },
                "typography": {
                        "fontFamily": "var( --wp--preset--font-family--primary )",
                        "fontSize": "var( --wp--preset--font-size--xl )",
                        "lineHeight": "var( --wp--custom--line-height--xl )"
                },
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"top": "var( --wp--preset--spacing--20 )",
								"bottom": "var( --wp--preset--spacing--20 )",
								"left": "var( --wp--preset--spacing--60 )",
								"right": "var( --wp--preset--spacing--60 )"
							}
						}
					}
				}
			},
			"core/code": {
				"border": {
					"color": "var( --wp--preset--color--neutral-400 )",
					"style": "solid",
					"width": "1px"
				},
				"spacing": {
					"padding": "var( --wp--preset--spacing--80 )"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--mono )",
					"fontSize": "var( --wp--preset--font-size--base )"
				},
				"css": "& code {\n\toverflow: auto;\n\tword-wrap: normal;\n\twhite-space: revert;\n}"
			},
			"core/image": {
				"css": "& img { box-shadow: var( --wp--preset--shadow--sm ); }"
			},
			"core/post-featured-image": {
				"css": "& img { box-shadow: var( --wp--preset--shadow--sm ); }"
			},
			"core/navigation": {
				"elements": {
					"link": {
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var( --wp--preset--color--neutral-400 )",
						"style": "solid",
						"width": "var( --wp--preset--spacing--10 )"
					}
				},
				"spacing": {
					"padding": {
						"left": "var( --wp--preset--spacing--40 )"
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var( --wp--preset--color--neutral-900 )"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": "var( --wp--preset--color--primary-800 )"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": "var( --wp--preset--color--primary-800 )"
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/table": {
				"border": {
					"color": "var( --wp--preset--color--neutral-400 )"
				},
				"typography": {
					"fontSize": "var( --wp--preset--font-size--lg )",
					"lineHeight": "var( --wp--custom--line-height--lg )"
				}
			}
		},
                "elements": {
			"button": {
				":focus": {
					"color": {
						"background": "var( --wp--preset--color--primary-800 )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"color": {
						"background": "var( --wp--preset--color--primary-800 )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var( --wp--preset--color--primary-500 )",
					"text": "var( --wp--preset--color--white )"
				},
				"spacing": {
					"padding": {
						"top": "var( --wp--preset--spacing--20 )",
						"bottom": "var( --wp--preset--spacing--20 )",
						"left": "var( --wp--preset--spacing--60 )",
						"right": "var( --wp--preset--spacing--60 )"
					}
				},
				"typography": {
					"fontSize": "var( --wp--preset--font-size--lg )",
					"lineHeight": "var( --wp--custom--line-height--lg )"
				}
			},
			"caption": {
				"color": {
					"text": "var( --wp--preset--color--neutral-700 )"
				},
				"typography": {
					"fontSize": "var( --wp--preset--font-size--lg )"
				}
			},
			"cite": {
				"color": {
					"text": "var( --wp--preset--color--neutral-700 )"
				},
				"typography": {
					"fontSize": "0.9375em",
					"fontStyle": "normal"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--headings )",
					"fontWeight": "400"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--5-xl )",
					"lineHeight": "var( --wp--custom--line-height--5-xl )"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--4-xl )",
					"lineHeight": "var( --wp--custom--line-height--4-xl )"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--3-xl )",
					"lineHeight": "var( --wp--custom--line-height--3-xl )"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--2-xl )",
					"lineHeight": "var( --wp--custom--line-height--2-xl )"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xl )",
					"lineHeight": "var( --wp--custom--line-height--xl )",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--lg )",
					"lineHeight": "var( --wp--custom--line-height--lg )",
					"textTransform": "uppercase"
				}
			},
                        "link": {
                                "color": {
                                        "text": "var( --wp--preset--color--primary-600 )"
                                },
				":focus": {
					"color": {
						"text": "var( --wp--preset--color--primary-800 )"
					}
				},
				":hover": {
					"color": {
						"text": "var( --wp--preset--color--primary-800 )"
					}
				}
                        }
		}
        },
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		}
	]
}
